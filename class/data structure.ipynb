    "def w(name):\n",
    "    try:\n",
    "        hand=open(name)\n",
    "    except:\n",
    "        print('file cannot be opened')\n",
    "        exit()\n",
    "    count=dict()\n",
    "    for line in hand:\n",
    "        words = line.split()\n",
    "        for word in words:\n",
    "            if word not in count:\n",
    "                count[word] = 1\n",
    "            else:\n",
    "                count[word]+=1\n",
    "        return(count)\n",
    "count=w('data.json') "
   ]
